{
  "version": 2,
  "name": "sistema-entrega-sobral",
  "buildCommand": "echo 'No build needed - static files'",
  "outputDirectory": "public",
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/admin",
      "destination": "/admin-login.html"
    },
    {
      "source": "/app",
      "destination": "/delivery-app-complete.html"
    },
    {
      "source": "/secure-v2",
      "destination": "/delivery-app-complete-secure-v2.html"
    },
    {
      "source": "/secure",
      "destination": "/delivery-app-complete-secure.html"
    }
  ]
}